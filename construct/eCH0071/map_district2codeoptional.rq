# Maps the optional fields to respective URIs
# Only includes Abolition entries

INSERT { GRAPH <http://example.org/inference> {
     ?disturi gont:abolitionMode ?districtAbolitionModeUri .

}}
WHERE {
  ?disturi a gont:DistrictEntityVersion;
    eCH-0071:districtAbolitionModeId ?districtAbolitionMode .
  
  ?districtAbolitionModeUri a skos:Concept ;
    skos:notation ?districtAbolitionMode .
}